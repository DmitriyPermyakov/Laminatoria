{
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "ng serve",
			"type": "pwa-chrome",
			"request": "launch",
			"preLaunchTask": "npm: start",
			"url": "http://localhost:4200/",
			"skipFiles": [
				"node_modules/**/*.js",
				"lib/**/*.js",
				"zone.js",
				"runtime.js",
				"polyfills.js",
				"vendor.js",
				"analytics.js"
			]
		},
		{
			"name": "ng test",
			"type": "chrome",
			"request": "launch",
			"preLaunchTask": "npm: test",
			"url": "http://localhost:9876/debug.html"
		}
	]
}
